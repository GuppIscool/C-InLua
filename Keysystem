-- CONFIG
local correctKey = "AV!9MP}d7I/hQ"EL"
local correctKey = "*GXE>_>s2pwNXo}S"
local correctKey = "[.$rwPF+F)Pao<RX"
local correctKey = "wS4%UK;xv)J>s;kW"
local scriptURL = "https://raw.githubusercontent.com/GuppIscool/C-InLua/refs/heads/main/Main" -- Replace with actual URL

-- UI for key entry
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextBox = Instance.new("TextBox")
local Button = Instance.new("TextButton")
local Message = Instance.new("TextLabel")

ScreenGui.Parent = game.CoreGui
Frame.Parent = ScreenGui
TextBox.Parent = Frame
Button.Parent = Frame
Message.Parent = Frame

-- Styling
Frame.Size = UDim2.new(0, 300, 0, 150)
Frame.Position = UDim2.new(0.5, -150, 0.5, -75)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.BorderSizePixel = 0

TextBox.Size = UDim2.new(0, 280, 0, 30)
TextBox.Position = UDim2.new(0, 10, 0, 10)
TextBox.PlaceholderText = "Enter your key..."

Button.Size = UDim2.new(0, 280, 0, 30)
Button.Position = UDim2.new(0, 10, 0, 50)
Button.Text = "Submit"
Button.BackgroundColor3 = Color3.fromRGB(60, 130, 255)
Button.TextColor3 = Color3.new(1, 1, 1)

Message.Size = UDim2.new(0, 280, 0, 30)
Message.Position = UDim2.new(0, 10, 0, 90)
Message.TextColor3 = Color3.new(1, 0.6, 0.6)
Message.Text = ""

-- Key validation logic
Button.MouseButton1Click:Connect(function()
    local enteredKey = TextBox.Text
    if enteredKey == correctKey then
        Message.Text = "Key valid! Loading script..."
        loadstring(game:HttpGet(scriptURL))()
        ScreenGui:Destroy()
    else
        Message.Text = "‚ùå Invalid key. Try again."
    end
end)
